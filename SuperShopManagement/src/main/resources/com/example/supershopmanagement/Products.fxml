      </AnchorPane>
      <Button fx:id="updateBtn" layoutX="232.0" layoutY="581.0" mnemonicParsing="false" onAction="#onProductClick" prefHeight="37.0" prefWidth="173.0" style="-fx-background-color: #03964d;" text="Add Product" textFill="WHITE">
